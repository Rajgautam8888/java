class HEllo{
    public static void main(string[]args){
        int a=7;
        switch (a){
            case 1;
            case 2;
            case 3;
            case 4;
            System.out.println("hello");
            break;
            case 5;
            System.out.println("hii");
            break;
            default;
            System.out.println("hi switch case");

        }
    }
}